<h1>Sign Up</h1>

<%= "Must include email and password" if @user && @user.errors.any? %>
<div class="btn-group">
<form action='/travelers' method="post">
  <h4>Username:
  <input type="text" id="username" name= "username">
  Password:
  <input type="password" id="password" name="password"></h4>
  <h4>First Name:
  <input type="text" id="first_name" name= "first_name">
  Last Name:
  <input type="text" id="last_name" name= "last_name"></h4>
  <h4>Date of Birth:
  <input type="text" id="dob" name= "dob">
  Home Airport:
  <input type="text" id="home_airport" name= "home_airport"></h4>
  <h4>Street Address:
  <input type="text" id="street_address" name= "street_address"></h4>
  <h4>City:
  <input type="text" id="city" name= "city">
  State:
  <input type="text" id="state" name= "state">
  Zip:
  <input type="text" id="zip" name= "zip"></h4>
  <button id="submit" type="submit">Sign Up</button>
</form>
</div>
